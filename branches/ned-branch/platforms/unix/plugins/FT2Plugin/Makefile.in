# default Makefile for Unix Squeak plugins

[make_cfg]
[make_plg]

XINCLUDES       = [includes] $(FT2_CFLAGS)
OBJS            = [targets]
TARGET          = [target]
PLIBS           = [plibs] $(FT2_LIBS)

[make_inc]

$(TARGET) : $(OBJS) Makefile
	$(LINK) $(TARGET) $(OBJS) $(PLIBS)

[make_targets]

.force : 
